<dialog layout="GridBagLayout" size="200">
	<vbox>
		<gridbagconstraints
			gridx="0"
			gridy="0"
			weightx="1"
			weighty="1"
			gridheight="1"
			anchor="GridBagConstraints.NORTHWEST"
			fill="GridBagConstraints.BOTH"
			/>
		<hbox>
			<label text="Set Status" />
			<combobox id="status" />
			<glue />
		</hbox>
	</vbox>
</dialog>
