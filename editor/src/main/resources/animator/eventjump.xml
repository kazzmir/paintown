<panel layout="GridBagLayout" size="200">
	<vbox>
		<gridbagconstraints
			gridx="0"
			gridy="0"
			weightx="1"
			weighty="1"
			gridheight="1"
			anchor="GridBagConstraints.NORTHWEST"
			fill="GridBagConstraints.BOTH"
			/>
		<hbox>
			<label text="Set X" />
			<panel id="x" />
			<glue />
		</hbox>
		<hbox>
			<label text="Set Y" />
			<panel id="y" />
			<glue />
		</hbox>
		<hbox>
			<label text="Set Z" />
			<panel id="z" />
			<glue />
		</hbox>
	</vbox>
</panel>
